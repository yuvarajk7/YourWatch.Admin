<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="YourWatch.Admin.Mobile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:YourWatch.Admin.Mobile"
    xmlns:views="clr-namespace:YourWatch.Admin.Mobile.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="YourWatch.Admin.Mobile">
    
    <Shell.TitleView>
        <views:LogoView/>
    </Shell.TitleView>
    
    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:EventOverviewPage}"
        Route="overview" /> 

</Shell>
